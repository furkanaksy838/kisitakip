{
  "name": "kisitakip",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2.1.1",
    "@sap/cds": "^9.1.0",
    "@sap/hana-client": "^2.25.29",
    "@sap/xsenv": "^5.6.1",
    "chokidar": "^4.0.3",
    "express": "^4.21.2",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9.1.1",
    "@sap/ux-ui5-tooling": "^1.18.2",
    "@ui5/cli": "^4.0.19",
    "cds-plugin-ui5": "^0.13.2",
    "mbt": "^1.2.34",
    "rimraf": "^6.0.1",
    "ui5-task-zipper": "^3.4.2"
  },
  "scripts": {
    "start": "cds-serve all",
    "watch-peopleapp": "cds watch --open com.kisitakip.peopleapp/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy kisitakip --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-project1": "cds watch --open stajui.project1/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "sapux": [
    "app/admin-books",
    "app/browse",
    "app/peopleapp"
  ],
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "html5-runtime": true,
      "portal": true
    }
  },
  "workspaces": [
    "app/*"
  ]
}
